{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },
      {
        "group": "asdk",
        "src" : "asdk",
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },
      {
        "group": "operator",
        "src" : "operator",
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },
      {
        "group": "partner",
        "src" : "partner",
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },      
      {
        "group": "user",
        "src" : "user",
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "group": "all",
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.svg"
        ],
        "exclude": [
          "**/obj/**",
          "_themes/**",
          "_themes.pdf/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "feedback_system": "GitHub",
      "feedback_github_repo": "MicrosoftDocs/azure-stack-docs",
      "feedback_product_url": "https://feedback.azure.com/forums/344565-azure-stack",
      "breadcrumb_path": "/azure-stack/breadcrumb/toc.json",
      "extendBreadcrumb": true,
      "_op_documentIdPathDepotMapping": {
        "./": {
          "depot_name": "Azure.azure-documents",
          "folder_relative_path_in_docset": "articles/azure-stack/"
        }
      },
      "manager":"femila",
      "ms.service": "azure-stack"          
    },
    "fileMetadata": {
      "titleSuffix": {
        "**/*.md": "Azure Stack Hub",
        "asdk/**/*.md": "Azure Stack Hub Development Kit (ASDK)"
      },
      "monikerRange": {
        "asdk/**": ">=azs-1802",
        "hybrid/**": ">=azs-1910",
        "operator/**": ">=azs-1802",
        "partner/**": ">=azs-1802",
        "user/**": ">=azs-1802"
      }
    },
    "template": [],
    "dest": "AzureStack",
    "markdownEngineName": "markdig",
    "groups": {
      "asdk": {
        "dest": "asdk",
        "moniker_range": ">=azs-1802"
      },
      "operator": {
        "dest": "operator",
        "moniker_range": ">=azs-1802"
      },
      "partner": {
        "dest": "partner",
        "moniker_range": ">=azs-1802"
      },
      "user": {
        "dest": "user",
        "moniker_range": ">=azs-1802"
      }                  
    }
  }
}
